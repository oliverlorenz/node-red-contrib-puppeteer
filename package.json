{
  "name": "node-red-contrib-puppeteer",
  "version": "0.2.0",
  "description": "A fork from Oliver Lorenz's node-red-contrib-puppeteer repo with some extra sauce. Allows controlling headless chrome from NodeRED. For original repo head over to https://github.com/oliverlorenz/node-red-contrib-puppeteer \n",
  "main": "index.js",
  "scripts": {},
  "author": "Shubham Mishra <shubham.root@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "puppeteer": "^1.1.1"
  },
  "bugs": {
    "url": "https://github.com/mayahq/maya-red-puppeteer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mayahq/maya-red-puppeteer"
  },
  "keywords": [
    "chrome",
    "browser automation",
    "puppeteer"
  ],
  "node-red": {
    "nodes": {
      "puppeteer-browser-launch": "puppeteer/browser/launch.js",
      "puppeteer-browser-close": "puppeteer/browser/close.js",
      "puppeteer-browser-newPage": "puppeteer/browser/newPage.js",
      "puppeteer-page-goto": "puppeteer/page/goto.js",
      "puppeteer-page-screenshot": "puppeteer/page/screenshot.js",
      "puppeteer-page-content": "puppeteer/page/content.js",
      "puppeteer-page-click": "puppeteer/page/click.js",
      "puppeteer-page-focus": "puppeteer/page/focus.js",
      "puppeteer-page-waitFor": "puppeteer/page/waitFor.js",
      "puppeteer-page-close": "puppeteer/page/close.js",
      "puppeteer-page-keyboard-type": "puppeteer/page/keyboard/type.js",
      "puppeteer-page-keyboard-press": "puppeteer/page/keyboard/press.js",
      "puppeteer-page-querySelector": "puppeteer/page/querySelector.js"
    }
  }
}